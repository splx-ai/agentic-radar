<style>
.main {
    width: 60%;
    margin: auto;

    flex-direction: column;
    display: flex;
}

.copy-btn {
    background-color: #e7e7e7;
    border: 1px solid #ccc;
    border-radius: 3px;
    padding: 2px 3px;
    cursor: pointer;
    flex-shrink: 0;
    margin-left: 5px;
    line-height: 0;
    display: inline-flex;
    align-items: center;
    justify-content: center;
    min-width: 20px;
    min-height: 20px;
}

.copy-btn:hover {
    background-color: #d4d4d4;
}

.copy-btn:active {
    background-color: #c0c0c0;
}

.copy-btn svg {
    vertical-align: middle;
    pointer-events: none;
}

.copy-btn.copied .copy-icon-svg {
    display: none; /* Hide the copy icon */
}
.copy-btn.copied .check-icon-svg {
    display: inline-block !important;
    stroke: green;
}

.copy-btn:disabled {
    cursor: default;
    opacity: 0.6;
}

.cell-content-wrapper {
    display: flex;
    justify-content: space-between;
    align-items: center;
    width: 100%;
    gap: 5px;
}

.copy-text {
    flex-grow: 1;
    word-wrap: break-word;
    overflow-wrap: break-word;
}

</style>
